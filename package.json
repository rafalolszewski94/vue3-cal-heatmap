    "vue3",
    "vuejs",
    "vue3js",
    "vue-component",
    "vue3-component",
    "calendar",
    "heatmap"
  ],
  "main": "dist/vue3-cal-heatmap.umd.js",
  "module": "dist/vue3-cal-heatmap.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "author": {
    "name": "Volker Nauruhn",
    "email": "mensch.meier@live.de",
    "url": "razorness.de"
  },
